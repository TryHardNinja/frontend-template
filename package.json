{
  "name": "frontend-template",
  "version": "0.1.1",
  "description": "Template of frontend",
  "author": "kkoudev (Koichi Nagaoka)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kkoudev/frontend-template.git"
  },
  "engines": {
    "node": ">=9.7.0"
  },
  "scripts": {
    "start": "node ./node_scripts/tasks/clean.js && run-p watch:* server",
    "build:postcss": "node ./node_scripts/tasks/postcss.js",
    "build:imagemin": "node ./node_scripts/tasks/imagemin.js",
    "build:pug": "node ./node_scripts/tasks/pug.js",
    "build:eslint": "node ./node_scripts/tasks/eslint.js",
    "build:fusebox": "node ./node_scripts/tasks/fusebox.js",
    "build": "node ./node_scripts/tasks/clean.js && run-s build:*",
    "watch:postcss": "cross-env NODE_WATCH=true STYLE_GUIDE=true nodemon ./node_scripts/tasks/postcss.js",
    "watch:imagemin": "cross-env NODE_WATCH=true nodemon ./node_scripts/tasks/imagemin.js",
    "watch:pug": "cross-env NODE_WATCH=true nodemon ./node_scripts/tasks/pug.js",
    "watch:eslint": "cross-env NODE_WATCH=true nodemon ./node_scripts/tasks/eslint.js",
    "watch:fusebox": "cross-env NODE_WATCH=true nodemon ./node_scripts/tasks/fusebox.js",
    "server": "nodemon ./node_scripts/tasks/server.js",
    "product": "cross-env NODE_ENV=production run-s build",
    "guide": "cross-env STYLE_GUIDE=true node ./node_scripts/tasks/postcss.js"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "blazy": "^1.8.2",
    "blueimp-canvas-to-blob": "^3.14.0",
    "blueimp-load-image": "^2.17.1",
    "jp-postal-code": "^0.2.0",
    "jquery": "^3.2.1",
    "jquery-datetimepicker": "^2.5.14",
    "lodash.assign": "^4.2.0",
    "moment": "^2.20.1",
    "vue": "^2.5.13",
    "vuex": "^3.0.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "avoriaz": "^6.3.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "browser-sync": "^2.23.1",
    "chalk": "^2.3.0",
    "chokidar": "^2.0.0",
    "consolidate": "^0.15.0",
    "cross-env": "^5.1.3",
    "css-mqpacker": "^6.0.1",
    "csswring": "^6.0.2",
    "eslint": "^4.14.0",
    "eslint-watch": "^3.1.2",
    "fs-extra": "^5.0.0",
    "fuse-box": "^3.1.3",
    "glob": "^7.1.2",
    "imagemin": "^5.3.1",
    "imagemin-gifsicle": "^5.2.0",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^5.0.1",
    "imagemin-svgo": "^6.0.0",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
    "minimatch": "^3.0.4",
    "node-notifier": "^5.1.2",
    "nodemon": "^1.14.7",
    "npm-run-all": "^4.1.2",
    "postcss": "^6.0.14",
    "postcss-assets": "^5.0.0",
    "postcss-calc": "^6.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-functions": "^3.0.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested-props": "^2.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-selector-parser": "^3.1.1",
    "postcss-sorting": "^3.1.0",
    "postcss-sprites": "^4.2.1",
    "postcss-style-guide": "^0.14.0",
    "postcss-webfont": "^1.1.0",
    "precss": "^3.1.2",
    "pug": "^2.0.0-rc.4",
    "sanitize.css": "^5.0.0",
    "stylelint": "^9.1.1",
    "stylelint-config-sugarss-recommended": "^1.1.0",
    "sugarss": "^1.0.1",
    "typescript": "^2.6.2",
    "unquote": "^1.1.1",
    "vue-hot-reload-api": "^2.2.4",
