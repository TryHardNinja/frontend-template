{
  "name": "frontend-template",
  "version": "0.0.1",
  "description": "Template of frontend",
  "author": "kkoudev (Koichi Nagaoka)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kkoudev/frontend-template.git"
  },
  "scripts": {
    "start": "node ./node_scripts/tasks/clean.js && run-p watch:* server",
    "build:postcss": "node ./node_scripts/tasks/postcss.js",
    "build:imagemin": "node ./node_scripts/tasks/imagemin.js",
    "build:pug": "node ./node_scripts/tasks/pug.js",
    "build:eslint": "node ./node_scripts/tasks/eslint.js",
    "build:fusebox": "node ./node_scripts/tasks/fusebox.js",
    "build": "node ./node_scripts/tasks/clean.js && run-s build:*",
    "watch:postcss": "cross-env NODE_WATCH=true STYLE_GUIDE=true nodemon ./node_scripts/tasks/postcss.js",
    "watch:imagemin": "cross-env NODE_WATCH=true nodemon ./node_scripts/tasks/imagemin.js",
    "watch:pug": "cross-env NODE_WATCH=true nodemon ./node_scripts/tasks/pug.js",
    "watch:eslint": "cross-env NODE_WATCH=true nodemon ./node_scripts/tasks/eslint.js",
    "watch:fusebox": "cross-env NODE_WATCH=true nodemon ./node_scripts/tasks/fusebox.js",
    "server": "nodemon ./node_scripts/tasks/server.js",
    "product": "cross-env NODE_ENV=production run-s build",
    "guide": "cross-env STYLE_GUIDE=true node ./node_scripts/tasks/postcss.js"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "dependencies": {
